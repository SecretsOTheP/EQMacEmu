# automatic eol conversions
* text=auto

*.c text eol=crlf
*.cc text eol=crlf
*.cpp text eol=crlf
*.h text eol=crlf
*.hpp text eol=crlf
*.txt text eol=crlf
*.pl text eol=crlf
*.sql text eol=crlf
*.xml text eol=crlf
*.yaml text eol=crlf
*.conf text eol=crlf
*.resx text eol=crlf
*.cs text eol=crlf
*.ini text eol=crlf
*.settings text eol=crlf
*.html text eol=crlf
*.css text eol=crlf
*.js text eol=crlf
*.types text eol=crlf

*.vcproj text merge=union
*.csproj text merge=union
*.sln text merge=union eol=crlf